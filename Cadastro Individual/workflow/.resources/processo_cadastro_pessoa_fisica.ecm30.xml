&#xd;
      params: dados,&#xd;
    };&#xd;
&#xd;
    hAPI.setTaskComments(&#xd;
      getValue(&quot;WKUser&quot;),&#xd;
      getValue(&quot;WKNumProces&quot;),&#xd;
      0,&#xd;
      JSON.stringify(data)&#xd;
    );&#xd;
&#xd;
    clientOauth = clientService.invoke(JSON.stringify(data));&#xd;
&#xd;
