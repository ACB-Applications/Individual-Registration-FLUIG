        else&#xd;
          hAPI.setCardValue(&quot;RespostaIntegracao&quot;, result.tt - msg[0].codFornec);&#xd;
      } else {&#xd;
        hAPI.setCardValue(&quot;codFornecedorIntegracao&quot;, &quot;&quot;);&#xd;
        hAPI.setCardValue(&#xd;
          &quot;RespostaIntegracao&quot;,&#xd;
          clientOauth.getHttpStatusResult() +&#xd;
            &quot; - &quot; +&#xd;
            clientOauth.getDescription()&#xd;
        );&#xd;
      }&#xd;
