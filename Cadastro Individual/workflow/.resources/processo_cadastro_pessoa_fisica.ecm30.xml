  v_telefonecelular = v_telefonecelular.replace(&quot;-&quot;, &quot;&quot;);&#xd;
&#xd;
  var v_CEP = parseInt(hAPI.getCardValue(&quot;CEP&quot;) + &quot;&quot;);&#xd;
  var v_estado = hAPI.getCardValue(&quot;estado&quot;) + &quot;&quot;;&#xd;
  var v_cidade = hAPI.getCardValue(&quot;cidade&quot;) + &quot;&quot;;&#xd;
