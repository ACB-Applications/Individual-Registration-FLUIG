&#xd;
  try {&#xd;
    var clientService = fluigAPI.getAuthorizeClientService();&#xd;
&#xd;
    var dados = {&#xd;
      &quot;tt-fornecedor&quot;: [&#xd;
        {&#xd;
          cCgc: v_CPF,&#xd;
          cNome: v_nomecompleto,&#xd;
          nomeAbrev: v_nomeabreviado,&#xd;
          cEmail: v_email,&#xd;
          cFoneCel: v_telefonecelular,&#xd;
          cFoneFixo: v_telefonefixo,&#xd;
          iCep: v_CEP,&#xd;
          cUF: v_estado,&#xd;
          cCicade: v_cidade,&#xd;
